{
    "name": "bcc/resque-bundle",
    "type": "symfony-bundle",
    "description": "BCCResqueBundle",
    "keywords": ["resque job queue management"],
    "homepage": "http://github.com/michelsalib/BCCResqueBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Salib",
            "email": "michelsalib@hotmail.com",
            "homepage": "http://michelsalib.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.0,<3.0",
        "chrisboulton/php-resque": "dev-recurring",
        "chrisboulton/php-resque-scheduler": "dev-recurring"
    },
    "repositories": [
	{
            "type": "vcs",
            "url": "git@github.com:rafaelcalleja/php-resque"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rafaelcalleja/php-resque-scheduler"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "BCC\\ResqueBundle": "" }
    },
    "target-dir": "BCC/ResqueBundle"
}
